<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    ><!-- tools:context=".MainActivity"-->


    <include android:id="@+id/miActionBar"
        layout="@layout/actionbar">
        </include>

    <ImageView
        android:id="@+id/ivEstrella"
        android:layout_width="@dimen/logos_actionbar"
        android:layout_height="@dimen/logos_actionbar"
        android:background="@drawable/baseline_star_black_18dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/margen_ivHueso_Amarillo"
        android:elevation="@dimen/elevacion_btnEstrella"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginRight="@dimen/margen_derechoBtnEstrella"
        android:onClick="IraSegundaActividad"
        />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvContactos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/miActionBar"
        android:layout_marginBottom="@dimen/margen_cvInferior"
        >

    </androidx.recyclerview.widget.RecyclerView>

